<%= form_tag search_representatives_path, method: :get, class: "search-bar-form" do %>
  <%= text_field_tag :query,
    params[:query],
    class: "searchbar-representatives address-input",
    style: "height: auto;",
    placeholder: "Entrez votre adresse"
  %>
  <%= submit_tag "Chercher", class: "btn-search margin-l-minus-50" %>
<% end %>



